<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http:mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whj.dao.PaperDao">
    <insert id="addPaper" useGeneratedKeys="true" keyProperty="sid">
        insert into paper(pid,eid,sid,cno,scontent,sa,sb,sc,sd,skey)
        values(default,#{eid},#{sid},#{cno},#{scontent},#{sa},#{sb},#{sc},#{sd},#{skey})
    </insert>

    <select id="getByEid" resultType="com.whj.pojo.Paper">
        select * from paper where eid=#{eid}
    </select>

    <delete id="deleteByEid">
        delete from paper where eid=#{eid}
    </delete>
</mapper>